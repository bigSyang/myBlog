{
    "index_name": "helpdocs", // 填写自己的索引名称
    "start_urls": ["https://yangsiyang1990.github.io/myBlog/"], // 填写自己的网站地址
    "rateLimit": 8,
    "maxDepth": 10,
    "selectors": {
      "lvl0": {
        "selector": "",
        "defaultValue": "Documentation"
      },
      "lvl1": ".content h1",
      "lvl2": ".content h2",
      "lvl3": ".content h3",
      "lvl4": ".content h4",
      "lvl5": ".content h5",
      "content": ".content p, .content li",
      "lang": {
        "selector": "/html/@lang",
        "type": "xpath",
        "global": true
      }
    },
    "selectors_exclude": [
      "aside",
      ".page-footer",
      ".next-and-prev-link",
      ".table-of-contents"
    ],
    "custom_settings": {
      "attributesForFaceting": ["lang", "tags"]
    },
    "js_render": true
  }
  